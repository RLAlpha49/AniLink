{
  "name": "anilink-api-wrapper",
  "version": "1.29.0",
  "description": "API wrapper for AniManga sites such as Anilist, MyAnimeList, Kitsu.",
  "main": "dist/AniLink.js",
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && tsc",
    "generate-docs": "typedoc --entryPoints ./src --entryPointStrategy expand --out docs --plugin typedoc-theme-hierarchy --theme hierarchy && node add-ga.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "anilist",
    "myanimelist",
    "kitsu",
    "api",
    "wrapper",
    "anilist-api",
    "anilist-query",
    "anilist-mutation",
    "anilist-graphql"
  ],
  "author": "RLAlpha49",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.1",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "standard": "^17.1.0",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.26.0",
    "typedoc-theme-hierarchy": "^5.0.3",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "axios": "^1.6.8"
  }
}
